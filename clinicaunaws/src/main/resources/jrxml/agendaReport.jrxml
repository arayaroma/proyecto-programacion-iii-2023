	<queryString>
		<![CDATA[SELECT U."NAME" AS DOCTOR_NAME,
	U."FIRSTLASTNAME" AS DOCTOR_FIRSTLASTNAME,
	U."SECONDLASTNAME" AS DOCTOR_SECONDTLASTNAME,
	D."IDCARD" AS DOCTOR_IDCARD,
	MA."SCHEDULEDDATE" AS APPOINTMENT_DATE,
	MA."REASON" AS APPOINTMENT_REASON,
	MA."STATE",
	MA."SCHEDULEDTIME",
	PA."NAME",
	PA."FIRSTLASTNAME",
	PA."SECONDLASTNAME",
	MA."PATIENTPHONENUMBER"
FROM "TBL_DOCTOR" D
	JOIN "TBL_USER" U ON 
	 D."ID" = U."ID" 
	JOIN "TBL_AGENDA" A ON 
	 D."ID" = A."DOCTOR" 
	JOIN "TBL_MEDICAL_APPOINTMENT" MA ON 
	 A."ID" = MA."AGENDA" 
	JOIN "TBL_PATIENT" PA ON 
	 MA."PATIENT" = PA."ID" 
WHERE 
	 D."ID" = $P{doctorId} 
	 AND A."AGENDADATE" BETWEEN $P{startDate} AND $P{endDate}]]>
	</queryString>
